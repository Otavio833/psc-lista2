programa {
  funcao inicio() {
    real precolitro, litrosvendidos , totalapagar
    escreva ( " digite o preço do litro da gasolina :\n " )
    leia (precolitro )
    escreva( "digite a quantidade de litros vendidos:\n " )
    leia(litrosvendidos)
    totalapagar  = precolitro * litrosvendidos
    escreva("o valor total a ser pago é: ", totalapagar)
  }
}
